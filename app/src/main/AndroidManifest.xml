<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaz.kazkoreanapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShoppingCart">
        <activity
            android:name="com.kaz.kazkoreanapp.CompleteOrderActivity"
            android:exported="false" />
        <activity android:name="com.kaz.kazkoreanapp.activities.ProductDetailsActivity" />
        <activity android:name="com.kaz.kazkoreanapp.activities.ProfileActivity" />
        <activity android:name="com.kaz.kazkoreanapp.activities.SettingsActivity" />
        <activity android:name="com.kaz.kazkoreanapp.activities.DetailedActivity" />
        <activity android:name="com.kaz.kazkoreanapp.activities.WelcomeActivity" />
        <activity android:name="com.kaz.kazkoreanapp.activities.IntroActivity"
            android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name="com.kaz.kazkoreanapp.activities.RegistrationActivity" />
        <activity android:name="com.kaz.kazkoreanapp.activities.LoginActivity" />
        <activity
            android:name="com.kaz.kazkoreanapp.ItemOrderActivity"
            android:exported="false"
            android:parentActivityName="com.kaz.kazkoreanapp.ItemListActivity" />
        <activity
            android:name="com.kaz.kazkoreanapp.MyOrderActivity"
            android:exported="false" />
        <activity
            android:name="com.kaz.kazkoreanapp.ItemListActivity"
            android:exported="false" />
        <activity
            android:name="com.kaz.kazkoreanapp.MainActivity"
            android:exported="false">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>